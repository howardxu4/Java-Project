				<Name>start</Name>				<Icon>Resources/images/buttons/rb.gif</Icon>				<PressedIcon>Resources/images/buttons/rbp.gif</PressedIcon>
				<RolloverIcon>Resources/images/buttons/rbr.gif</RolloverIcon>
				<RolloverSelectedIcon>Resources/images/buttons/rbrs.gif</RolloverSelectedIcon>
