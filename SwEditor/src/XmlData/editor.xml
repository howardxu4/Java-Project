							<Icon>resources/images/splitpane/earth.jpg</Icon>	
							<ToolTipText>this is a .. ?</ToolTipText>		
						</Label>
						</ScrollPane>
					</Panel>
				</InternalFrame>
				<InternalFrame>
					<Title>Working Window</Title>
					<Maximizable>yes</Maximizable>
