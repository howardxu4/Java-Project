							<SaveId>delTree</SaveId>
							<Root>root</Root>
						</Tree>
						</ScrollPane>
					</Panel>
				</InternalFrame>
				<InternalFrame>
					<Title>Trace Window</Title>
					<Maximizable>yes</Maximizable>
					<Resizable>1</Resizable>
					<Iconifiable>2</Iconifiable>
					<Rectangle>
						<X>100</X>
						<Y>100</Y>
						<Height>300</Height>
						<Width>400</Width>
					</Rectangle>
					<Panel>
						<Layout Type="Border"></Layout>
						<ScrollPane Position="Center">
						<TextArea>
							<SaveId>myPad</SaveId>
							<Text>start...</Text>
						</TextArea>
						</ScrollPane>
					</Panel>
				</InternalFrame>
				<InternalFrame>
					<Title>Debug Window</Title>
					<Maximizable>yes</Maximizable>
					<Resizable>1</Resizable>
					<Iconifiable>2</Iconifiable>
