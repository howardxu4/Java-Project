		        <SelectedIcon>resources/images/buttons/brdn.gif</SelectedIcon>
				</RadioButton>
			</Box>			</Layout>
		</Panel>		</Layout> 	</Panel>
 	</Layout>
 	</Panel>	</UIConfig>
